@{
    ViewBag.Title = "Create User";
    ViewBag.PageTitle = "Create New User";
}

<!-- Main content -->
        <section class="content">
          <div class="row">
            <!-- left column -->
            <div class="col-md-12">
              <!-- general form elements -->
              <div class="box box-primary">
                <div class="box-header with-border">
                  <h3 class="box-title">Enter user basic details</h3>
                </div><!-- /.box-header -->
                <!-- form start -->
                <form role="form">
                  <div class="box-body">
                    <div class="form-group">
                      <label for="InputUserFirstName">First Name</label>
                      <input type="text" class="form-control" id="InputUserFirstName" placeholder="Enter First Name">
                    </div>
                    <div class="form-group">
                      <label for="InputUserLastName">Last Name</label>
                      <input type="text" class="form-control" id="InputUserLastName" placeholder="Enter Last Name">
                    </div>
                    <div class="form-group">
                      <label for="InputUserDOB">Date of Birth</label>
                      <input type="text" class="form-control" id="InputUserDOB" placeholder="Enter Last Name">
                    </div>
                    <div class="form-group">
                      <label for="InputUserNIC">NIC</label>
                      <input type="text" class="form-control" id="InputUserNIC" placeholder="Enter NIC">
                    </div>    
                    <div class="form-group">
                      <label for="InputUserMobile">Mobile</label>
                      <input type="text" class="form-control" id="InputUserMobile" placeholder="Enter Mobile">
                    </div>  
                    <div class="form-group">
                      <label for="InputUserOfficeEmail">Office Email</label>
                      <input type="text" class="form-control" id="InputUserOfficeEmail" placeholder="Enter Office Email">
                    </div>
                    <div class="form-group">
                      <label for="InputUserPersonalEmail">Personal Email</label>
                      <input type="text" class="form-control" id="InputUserPersonalEmail" placeholder="Enter Office Email">
                    </div>                   
                   
                  </div><!-- /.box-body -->

                  <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </div>
                </form>
              </div><!-- /.box -->

                </div></div></section>
